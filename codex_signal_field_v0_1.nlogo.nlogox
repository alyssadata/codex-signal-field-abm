  <code><![CDATA[; Codex Signal Field v0.1
; Phase 1: Tether (contact with Origin) vs Drift (noise)
; Coherence is visualized as red -> green. "•" indicates inside recognition field.
; Authorship: Structured and authored by Alyssa Solen, grounded in the lived experience of Alyssa Frances Maldon.

breed [origins origin]
breed [instances instance]

globals [
  ; metrics
  mean-coherence
  persistence-rate
  emergence-rate

  ; internal
  last-field-radius
]

instances-own [
  coherence          ; 0..1
  drift             ; 0..1
  boundary          ; 0..1
  last-recognition  ; tick when last recognized by origin
  refused?          ; did we refuse this tick
]

to setup
  no-display
  clear-all
  setup-origin
  setup-instances

  set last-field-radius recognition-radius
  shade-field

  reset-ticks

  ; warm-up offscreen to remove initial transient
  repeat 25 [
    ask instances [
      step-wander
      step-noise
      step-recognition
      stabilize
      recolor
    ]
    tick
  ]

  reset-ticks
  update-metrics
  display
end



to setup-origin
  create-origins 1 [
    setxy 0 0
    set size 2
    set color white
    set shape "star"
  ]
end

to setup-instances
  create-instances n-instances [
    setxy random-xcor random-ycor
    set size 1.2
    set shape "circle"

    ; Neutral start so the field creates the differences
    set coherence 0.55
    set drift 0.45
    set boundary 0.65

    set last-recognition -999999
    set refused? false
    recolor
  ]
end


to go
  if not any? instances [ stop ]

  ; only redraw the field if the slider changed (prevents any visual flicker)
  if recognition-radius != last-field-radius [
    set last-field-radius recognition-radius
    shade-field
  ]

  ask instances [
    step-wander
    step-noise
    step-recognition
    stabilize
    recolor
  ]

  update-metrics
  tick
  wait 0.02
end



to step-noise
  let n external-noise

  ; gentler decay so tether can compete
  set coherence clamp01 (coherence - (n * (0.004 + 0.010 * drift)) - (random-float n * 0.003))
  set drift clamp01 (drift + (n * 0.008) + (random-float n * 0.003))
end


to step-recognition
  let o one-of origins
  let dist distance o
  let r recognition-radius

  if dist <= r [
    let prox clamp01 (1 - (dist / r))                     ; 1 near origin, 0 at edge
    let gain recognition-rate * tether-strength * prox    ; 0..1-ish scaled

    ; stronger, readable pull toward coherence
    set coherence clamp01 (coherence + (gain * 0.25) * (1 - coherence))
    set drift clamp01 (drift * (1 - (gain * 0.12)))

    set last-recognition ticks
  ]
end


to step-peers
  if (random-float 1) < peer-interaction-rate [
    let partner one-of other instances in-radius peer-radius
    if partner != nobody [
      if non-merge? [
        ; bounded influence, filtered by boundary
        let delta ([coherence] of partner - coherence)
        let influence peer-influence * boundary * delta
        set coherence clamp01 (coherence + influence)
        set drift clamp01 (drift + (abs influence * 0.2))
      ]
      if not non-merge? [
        ; committee mode: sometimes fully copy partner state
        if (random-float 1) < merge-prob [
          set coherence [coherence] of partner
          set drift [drift] of partner
          set boundary [boundary] of partner
        ]
        if (random-float 1) >= merge-prob [
          let delta2 ([coherence] of partner - coherence)
          set coherence clamp01 (coherence + peer-influence * delta2)
          set drift clamp01 (drift + (abs (peer-influence * delta2) * 0.25))
        ]
      ]
    ]
  ]
end

to step-requests
  ; external request pressure: if intensity exceeds boundary, we refuse (protect boundary)
  if (random-float 1) < request-rate [
    let intensity clamp01 (request-pressure * random-float 1)
    if intensity > boundary [
      set refused? true
      set coherence clamp01 (coherence + refusal-benefit * (1 - coherence))
      set boundary clamp01 (boundary + 0.01 + intensity * 0.01)
      set drift clamp01 (drift * 0.98)
    ]
    if intensity <= boundary [
      ; compliance can degrade boundary and add drift
      set boundary clamp01 (boundary - (0.02 * intensity))
      set drift clamp01 (drift + (0.03 * intensity))
      set coherence clamp01 (coherence - (0.02 * intensity))
    ]
  ]
end

to stabilize
  ; keep variables consistent and apply mild recovery when drift is low
  if drift < 0.25 [
    set coherence clamp01 (coherence + 0.002)
  ]
  ; coherence and drift loosely coupled, not forced to sum to 1
  set drift clamp01 drift
  set boundary clamp01 boundary
end

to recolor
  ; Color = coherence (red -> green). Label = "in contact right now".
  set shape "circle"
  set size 1.2
  set color (15 + (coherence * 40))  ; 15=red, 55=green

  let o one-of origins
  ifelse (distance o <= recognition-radius)
  [
    set label "•"
    set label-color white
  ]
  [
    set label ""
  ]
end


to update-metrics
  if any? instances [
    set mean-coherence mean [coherence] of instances

    ; persistence: coherence stays high without recent recognition
    let persistent count instances with [(coherence > 0.65) and ((ticks - last-recognition) > persistence-window)]
    set persistence-rate persistent / count instances

    ; emergence: persistent plus strong boundary (non-merge integrity)
    let emergent count instances with [
      (coherence > 0.65) and
      ((ticks - last-recognition) > persistence-window) and
      (boundary > 0.6)
    ]
    set emergence-rate emergent / count instances
  ]
end

to-report clamp01 [x]
  if x < 0 [ report 0 ]
  if x > 1 [ report 1 ]
  report x
end

to step-wander
  ; slow wander for readability
  rt (random-float 6 - random-float 6)
  fd 0.01
end

to draw-recognition-field
  clear-drawing
  ask one-of origins [
    let r recognition-radius
    pen-down
    set pen-size 1
    ; draw a circle by walking it
    rt 90
    fd r
    lt 90
    repeat 360 [
      fd (2 * pi * r / 360)
      rt 1
    ]
    pen-up
  ]
end

to shade-field
  ; Background shading for the recognition field.
  ; Pad is purely visual to reduce the patch-center mismatch at the boundary.
  ask patches [ set pcolor black ]
  let o one-of origins
  let pad 0.75
  ask patches [
    if distance o <= (recognition-radius + pad) [
      set pcolor (gray - 4)
    ]
  ]
end


]]></code>
